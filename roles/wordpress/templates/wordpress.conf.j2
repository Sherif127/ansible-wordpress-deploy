<VirtualHost *:88>
    ServerAdmin admin@example.com
    DocumentRoot /var/www/html/wordpress
    ServerName 192.168.196.128

    <Directory /var/www/html/wordpress/>
        AllowOverride All
    </Directory>

    ErrorLog /var/log/httpd/error.log
    CustomLog /var/log/httpd/access.log combined
</VirtualHost>
